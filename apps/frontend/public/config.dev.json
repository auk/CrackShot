{
  "webapp": {
    "version": "0.0.0"
  },
  "services": {
    "auth": "https://tt.startext.ru/TimeTracker-auth",
    "rest": "http://127.0.0.1:8080"
  },
  "roles": {
    "admin": [
      "ROLE_TimeTracker"
    ],
    "user": []
  },
  "links": {
    "auth": {
      "url": "{AUTH_URL}/oauth/token"
    },
    "home": {
      "url": "/main"
    },
    "about": {
      "url": "/about"
    },
    "login": {
      "url": "/login"
    },
    "calculator": {
      "url": "/calculator"
    },
    "organizations": {
      "url": "/organizations",
      "api": {
        "get": "{REST_URL}/organizations"
      }
    },
    "organization": {
      "url": "/organization/:oid/view",
      "api": {
        "get": "{REST_URL}/organization/:oid",
        "create": "{REST_URL}/organization",
        "update": "{REST_URL}/organization/:oid",
        "delete": "{REST_URL}/organization/:oid"
      }
    },
    "users": {
      "url": "/users",
      "api": {
        "get": "{REST_URL}/users"
      }
    },
    "user": {
      "url": "/user/:uid",
      "api": {
        "get": "{REST_URL}/user"
      }
    },
    "training": {
      "url": "/training/:tid",
      "api": {
        "create": "{REST_URL}/training",
        "get": "{REST_URL}/training/:tid"
      }
    },
    "trainings": {
      "url": "/trainings",
      "api": {
        "get": "{REST_URL}/trainings/search"
      }
    }
  }
}